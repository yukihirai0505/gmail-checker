{
  "manifest_version": 2,

  "name": "Gmail Checker",
  "description": "Improving mail subject and content for Gmail",
  "version": "1.0.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "js": ["js/checker.js"]
    }
  ],
  "permissions": []
}
